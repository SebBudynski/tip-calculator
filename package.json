{
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "server": "node server.js",
    "sass": "node-sass -w styles/ -o styles/css/ --",
    "sync": "browser-sync start --proxy localhost:3000 --files '**/*.scss, **/*.html'",
    "start": "npm-run-all --parallel server sass sync"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
